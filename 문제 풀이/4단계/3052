import java.util.*;

public class Main {
    public static void main(String[] args) {
        int[] arr = new int[10];
        Scanner sc = new Scanner(System.in);
        
        for(int i = 0; i < 10; i++) {
            arr[i] = sc.nextInt() % 42;
        }
        
        Arrays.sort(arr);
        
        int cnt = 1;
        
        for(int i = 1; i < 10; i++) {
            if(arr[i - 1] != arr[i]) cnt++;
        }
        
        System.out.println(cnt);
    }
}
